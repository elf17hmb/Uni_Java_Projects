<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.fh_zwickau.propertyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="de.fh_zwickau.propertyapp.PropertyApp"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="de.fh_zwickau.propertyapp.PropertyList"
            android:label="@string/app_name">
        </activity>
        <activity android:name="de.fh_zwickau.propertyapp.PropertyDetails"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>
